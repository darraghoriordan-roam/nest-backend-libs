{
    "name": "@darraghor/nest-backend-libs",
    "version": "1.0.1",
    "description": "Some helpers for personal projects",
    "homepage": "https://github.com/darraghoriordan/nest-backend-libs",
    "repository": {
        "type": "git",
        "url": "https://github.com/darraghoriordan/nest-backend-libs"
    },
    "engines": {
        "node": ">=12"
    },
    "files": [
        "dist",
        "package.json",
        "README.md"
    ],
    "author": {
        "name": "Darragh ORiordan",
        "email": "darragh.oriordan@gmail.com",
        "url": "https://darraghoriordan.com"
    },
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "keywords": [
        "nestjs"
    ],
    "license": "ISC",
    "scripts": {
        "clean": "rm -Rf ./dist/",
        "build:ci": "npm run build",
        "build": "npm run clean && mkdir ./dist && tsc --project tsconfig.build.json",
        "lint": "npx eslint -c .eslintrc.js --ext .ts './src/**/*.ts' './tests/**/*.ts' --no-error-on-unmatched-pattern",
        "test": "export NODE_OPTIONS=\"--max-old-space-size=8192\" && npx jest --runInBand --color --reporters=default --no-cache --coverage=false --passWithNoTests",
        "pre-commit": "npx lint-staged",
        "prettier": "prettier --write src/*",
        "prepare": "husky install",
        "release": "npx semantic-release",
        "semantic-release": "semantic-release"
    },
    "dependencies": {
        "@nestjs/common": "^7.6.18",
        "@nestjs/config": "0.6.3",
        "@nestjs/core": "^7.6.18",
        "@nestjs/passport": "^7.1.6",
        "@nestjs/platform-express": "^7.6.18",
        "@nestjs/swagger": "^4.8.2",
        "@nestjs/typeorm": "^7.1.5",
        "auth0": "^2.36.1",
        "dotenv": "^10.0.0",
        "class-transformer": "0.4.0",
        "class-validator": "0.13.1",
        "date-fns": "^2.23.0",
        "jwks-rsa": "^2.0.4",
        "nodemailer": "^6.6.3",
        "passport": "^0.4.1",
        "passport-jwt": "^4.0.0",
        "pg": "^8.7.1",
        "reflect-metadata": "^0.1.13",
        "res": "^0.4.0",
        "resource": "^0.8.1",
        "rimraf": "^3.0.2",
        "rxjs": "^7.3.0",
        "tslog": "^3.2.1",
        "swagger-ui-express": "^4.1.6",
        "typeorm": "^0.2.37"
    },
    "devDependencies": {
        "@darraghor/eslint-plugin-nestjs-typed": "3.2.0",
        "@commitlint/cli": "12.1.1",
        "@commitlint/config-conventional": "12.1.1",
        "@semantic-release/changelog": "5.0.1",
        "@semantic-release/exec": "5.0.0",
        "@nestjs/cli": "^7.6.0",
        "@nestjs/schematics": "^7.3.1",
        "@types/auth0": "^2.33.4",
        "@types/express": "^4.17.13",
        "@types/jest": "^26.0.24",
        "@types/node": "16.11.6",
        "@types/nodemailer": "^6.4.4",
        "@types/passport-jwt": "^3.0.6",
        "@types/supertest": "^2.0.11",
        "@typescript-eslint/eslint-plugin": "5.2.0",
        "@typescript-eslint/parser": "5.2.0",
        "eslint": "8.1.0",
        "eslint-config-prettier": "8.3.0",
        "eslint-plugin-eslint-comments": "3.2.0",
        "eslint-plugin-import": "2.25.2",
        "eslint-plugin-jest": "25.2.2",
        "eslint-plugin-jsdoc": "37.0.1",
        "eslint-plugin-prefer-arrow": "1.2.3",
        "eslint-plugin-promise": "5.1.1",
        "eslint-plugin-sonarjs": "0.10.0",
        "eslint-plugin-unicorn": "37.0.1",
        "husky": "6.0.0",
        "lint-staged": "11.0.0",
        "jest": "^27.0.6",
        "semantic-release": "17.4.3",
        "jest-create-mock-instance": "^1.1.0",
        "prettier": "^2.3.2",
        "ts-jest": "^27.0.4",
        "ts-loader": "^9.2.5",
        "ts-node": "^10.2.0",
        "tsconfig-paths": "^3.10.1",
        "typescript": "4.4.4"
    },
    "lint-staged": {
        "*.ts": "eslint --cache --fix",
        "*.{ts,js,css,md}": "prettier --write"
    },
    "release": {
        "branches": [
            "main"
        ]
    }
}
